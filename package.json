{
  "name": "runction",
  "version": "1.0.2",
  "description": "Usefull ramda functions.",
  "main": "src/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "npm test",
    "lint": "xo",
    "prettier": "prettier **/*.js",
    "test": "jest --coverage"
  },
  "keywords": [],
  "author": "thanos@thechatshop.com",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "xo": "^0.20.3"
  },
  "peerDependencies": {
    "ramda": "^0.25.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "xo --fix",
      "git add"
    ]
  },
  "xo": {
    "envs": [
      "jest"
    ],
    "extends": [
      "prettier"
    ],
    "rules": {
      "camelcase": [
        1
      ],
      "unicorn/filename-case": [
        "error",
        "camelCase"
      ]
    }
  }
}
